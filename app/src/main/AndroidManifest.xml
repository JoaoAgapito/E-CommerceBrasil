            android:name=".MainActivityItem5"
            android:exported="false" />
        <activity
            android:name=".MainActivityItem4"
            android:exported="false" />
        <activity
            android:name=".MainActivityItem3"
            android:exported="false"
            android:label="@string/title_activity_main_item3"
